<div class="container">
    <section class="col-md-12 content" id="home">
        <div class="col-lg-6 col-md-6 content-item tm-black-translucent-bg tm-logo-box">
            <i class="fa fa-snowflake-o fa-4x tm-logo"></i>
            <h1 class="text-uppercase">Catalyst</h1>

        </div>
        <form method="POST" action="/guardarUsuario">
            <div class="col-lg-6 col-md-6 content-item content-item-1 background tm-white-translucent-bg">
                <div class="form-group">
                    <input id="cedula" class="cedula" name="cedula" type="text" placeholder="Ingrese cedula" maxlength="10">
                </div>
                <div class="form-group">
                    <input id="nombres" class="nombres" name="nombres" type="text" placeholder="Ingrese nombres">
                </div> 
                <div class="form-group">
                    <input id="apellidos" class="apellidos" name="apellidos" type="text" placeholder="Ingrese apellidos">
                </div>
                <div class="form-group">
                    <input id="usuario" class="usuario" name="usuario" type="text" placeholder="Ingrese cedula">
                </div> 
                <div class="form-group">
                    <input id="clave" class="clave" name="clave" type="password"  placeholder="Ingrese cedula">
                </div>
                <div class="form-group">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancelar</button>
                    <button type="submit" class="btn btn-primary" >Guardar</button>
                </div> 
            </div>
        </form>
    </section>

</div>
